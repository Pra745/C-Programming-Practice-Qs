#include <stdio.h>

void main()
{
    int a,b,c;
    printf("Enter the lengths of the three sides of the triangle");
    scanf("%d", &a, &b, &c);
    if (a==b && b==c)
    printf("The triangle is Equilateral");
    else if (a==b || b==c || a==c)
    printf("The triangle is Isosceles");
    else
    printf("The triangle is Scalene");
    getch();
}
